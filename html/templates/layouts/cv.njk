{% extends "./with_preferences.njk" %}

{% block content_with_preferences %}
<!DOCTYPE html>
<html lang="en">
  <head>
    <title>CV</title>
    <meta charset="utf-8" />
    <meta name="keywords" content="Web-Developer, Resume, CV">
    <meta name="description" content="My Web-Develeoper Resume">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0" />
    <link href="./styles.css"
          rel="stylesheet"
          media="screen" />
    <link href="./styles.css"
          rel="stylesheet"
          media="print" />
  </head>
  <body>
    <div class="cv-content-wrapper  {{ "theme-dark" if theme == theme_dark else theme_light }}">

      {% block first_page %}
      {% endblock first_page %}

      {% block second_page %}
      {% endblock second_page %}

    </div>
  </body>
</html>
{% endblock content_with_preferences %}
